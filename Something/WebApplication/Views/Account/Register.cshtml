@model Service.ViewModels.AccountViewModels.RegisterViewModel
@using Service.ViewModels.Enums;
@{
    ViewData["Title"] = "Register";
    Layout = null;
}


<div id="login-root" />

<script type="text/javascript">
    window.authenticationPageParams = {
        pageType: @((int)PageTypeEnum.Register),
        email: "@Model.Email",
        failedAttempt: "@Model.FailedAttempt",
        serverError: "@Model.ServerError",
    }
</script>

<script src="~/dist/login.bundle.js"></script>
