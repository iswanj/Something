@using Common.Models;
@model Service.ViewModels.AccountViewModels.LoginViewModel
@inject SignInManager<ApplicationUser> SignInManager

@{
    ViewData["Title"] = "Log in";
    Layout = null;
}

<div id="login-root" />

<script type="text/javascript">
    window.authenticationPageParams = {
        email: "@Model.Email",
        failedAttempt: "@Model.FailedAttempt",
        serverError: "@Model.ServerError",
    }
</script>

<script src="~/dist/login.bundle.js"></script>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
